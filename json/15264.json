				strErrorMsg = "Please select "+strLabel+"." ;		
			}

			blError = true;
			break;
				
		}
		
		if (strValidationType.toUpperCase() == "I")
		{
			if (!isInteger(strValue))
			{
				if (!blMessage)
	//				strErrorMsg = "Please enter whole number in " + strLabel + "\n e.g. 1,2,3...";
					strErrorMsg = "Please enter whole number in " + strLabel + "\n e.g. 1,2,3...";
					
