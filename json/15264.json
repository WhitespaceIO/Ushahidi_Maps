		}
	
		
		if (strValidationType.toUpperCase() == "S")
		{	
			if (strValue == "" || strValue == "undefined")
			{
				strErrorMsg = "Please select " + strLabel + ".";
				blError = true;
				break;				
			}
			else if(eval(strValue) == -1 || strValue == -1){
